{
  "manifest_version": 2,

  "name": "botb random vote",
  "version": "1.0",
  "description": "takes you to a random unvoted entry from major battles",

  "browser_action": {
      "default_title": "VOTE!"
  },

  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },

  "permissions": ["activeTab"]
}
